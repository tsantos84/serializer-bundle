{
    "name": "tsantos/serializer-bundle",
    "description": "Integrates TSantos Serializer in Symfony applications",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php":"^7.2",
        "symfony/config": "^4.0",
        "symfony/dependency-injection": "^4.0",
        "symfony/http-kernel": "^4.0",
        "tsantos/serializer": "^2.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.0",
        "symfony/stopwatch": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "TSantos\\SerializerBundle\\" : "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TSantos\\SerializerBundle\\Tests\\": "./tests"
        }
    },
    "authors": [
        {
            "name": "Tales Santos",
            "email": "tales.augusto.santos@gmail.com"
        }
    ]
}
